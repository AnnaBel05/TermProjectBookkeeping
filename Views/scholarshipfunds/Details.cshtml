@model TermProjectBookkeeping.scholarshipfund

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>scholarshipfund</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Дата формирования
        </dt>

        <dd>
            @Html.DisplayFor(model => model.formationdate)
        </dd>

        <dt>
            Сумма соц. стипендии
        </dt>

        <dd>
            @Html.DisplayFor(model => model.scholarshipsocial)
        </dd>

        <dt>
            Сумма стипендии на "хор"
        </dt>

        <dd>
            @Html.DisplayFor(model => model.scholarshipgreat)
        </dd>

        <dt>
            Сумма стипендии на "хор" и "отл"
        </dt>

        <dd>
            @Html.DisplayFor(model => model.scholarshipgreatperfect)
        </dd>

        <dt>
            Сумма стипендии на "отл"
        </dt>

        <dd>
            @Html.DisplayFor(model => model.scholarshipperfect)
        </dd>

        <dt>
            Базовая стипендия
        </dt>

        <dd>
            @Html.DisplayFor(model => model.basescholarship)
        </dd>

        <dt>
            Общая сумма
        </dt>

        <dd>
            @Html.DisplayFor(model => model.totalsum)
